FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      residuals;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

residuals
{
    type            		solverInfo;
    libs 			("libutilityFunctionObjects.so");


    fields 			(p U k omega);

    writeResidualFields 	false;
    writeControl    		timeStep;
    writeInterval   		1;
}

