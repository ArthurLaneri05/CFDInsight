FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "./initialConditions"



Hub.obj
{
    extractionMethod    extractFromSurface;

    extractFromSurfaceCoeffs
    {
        // - 0  : selects no edges
        // - 180: selects all edges
        includedAngle   150;
    }

    subsetFeatures
    {     
        nonManifoldEdges       no;

        openEdges       yes;
    }

        // Write features to stl format for postprocessing
        writeObj	no;
}

